{
  "expo": {
    "name": "Mixology",
    "privacy": "public",
    "slug": "Mixology",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "MixologyV2 is a mobile application that offers over 600 unique cocktails. Browse some popular drinks or save your favourites for later. MixologyV2 is A must have for any party.",
    "githubUrl": "https://github.com/mikrowil/mixologyV2"
  }
}
